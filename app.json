{
  "expo": {
    "name": "todoist",
    "slug": "todoist",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "hozayvestodoist",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.todoist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/todoist-logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.todoist"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/todoist-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/todoist-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store",
      "react-native-bottom-tabs",
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3a79c1df-7433-4e82-a000-9d01910f0244"
      }
    },
    "owner": "hozayves",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/3a79c1df-7433-4e82-a000-9d01910f0244"
    }
  }
}
